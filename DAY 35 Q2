#include <iostream>
using namespace std;

int main() 
{
    int  t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int arr[n];
        for(int i=0;i<n;i++)
        cin>>arr[i];
        int c,i;
        for(i=0,c=0;i<n;i++)
        {
            if(arr[i]%2!=0)
            c++;
        }
        if(c%2==0 && c!=0)
        cout<<"YES"<<endl;
        else
        cout<<"NO"<<endl;
        
    }
	return 0;
}
