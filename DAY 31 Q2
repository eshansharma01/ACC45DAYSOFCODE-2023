#include <iostream>
using namespace std;

int main() 
{
    int n,h,x;
    cin>>n>>h>>x;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int i;
    for(i=0;i<n;i++)
    {
        if(arr[i]>=(h-x))
        {
            cout<<"YES"<<endl;
            break;
        }
    }
    if(i==n)
    cout<<"NO"<<endl;
    
	return 0;
}
